{
  "name": "fhevm-universal-sdk",
  "version": "1.0.0",
  "description": "Universal FHEVM SDK with framework-agnostic design and comprehensive examples",
  "private": true,
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "build": "npm run build:sdk",
    "build:sdk": "cd packages/fhevm-sdk && npm run build",
    "install:all": "npm install && npm install --workspaces",
    "dev:nextjs": "cd examples/nextjs-example && npm run dev",
    "dev:vue": "cd examples/vue-example && npm run dev",
    "dev:node": "cd examples/node-example && npm start",
    "test": "npm run test:sdk",
    "test:sdk": "cd packages/fhevm-sdk && npm test"
  },
  "keywords": [
    "fhevm",
    "encryption",
    "privacy",
    "ethereum",
    "web3",
    "sdk",
    "react",
    "vue",
    "nextjs"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=7.0.0"
  }
}
